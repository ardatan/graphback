{
  "name": "graphql-migrations",
  "version": "0.10.0-rc2",
  "description": "Graphback database migrations",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "typescript": {
    "definition": "types/index.d.ts"
  },
  "dependencies": {
    "@graphback/core": "^0.16.0",
    "@graphql-inspector/core": "1.30.4",
    "chalk": "2.4.2",
    "glob": "7.2.3",
    "knex": "0.95.15",
    "node-emoji": "1.11.0",
    "pg": "7.18.2",
    "pino": "5.17.0",
    "sqlite3": "4.2.0"
  },
  "devDependencies": {
    "@types/chalk": "0.4.31",
    "@types/graphql": "14.5.0",
    "@types/node": "12.20.55",
    "ava": "2.4.0",
    "graphql": "^14.5.8",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "tslint": "5.20.1",
    "tsutils": "3.21.0",
    "typescript": "3.9.10"
  },
  "scripts": {
    "clean": "rimraf coverage dist types",
    "build": "tsc",
    "watch": "tsc -w ",
