{
  "name": "graphback-cli",
  "version": "0.10.0-rc2",
  "description": "Auto generate resolvers on top of the graphql and database of your choice",
  "main": "dist/index.js",
  "bin": {
    "graphback": "dist/index.js"
  },
  "types": "types/index.d.ts",
  "typescript": {
    "definition": "types/index.d.ts"
  },
  "author": "wtrocki@redhat.com",
  "scripts": {
    "clean": "rimraf coverage dist types",
    "build": "tsc && cp -Rf ./src/templates/resources ./dist/templates/resources",
    "cmd": "node ./dist/index.js",
    "watch": "tsc -w",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "ava",
    "pretest": "npm run build"
  },
  "ava": {
    "files": [
      "tests/**/*"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "engines": {
    "node": ">=10.15.3"
  },
  "devDependencies": {
    "@types/chalk": "2.2.0",
    "@types/inquirer": "9.0.7",
    "@types/js-yaml": "4.0.9",
    "@types/node": "20.14.1",
    "@types/node-emoji": "2.1.0",
    "@types/through": "0.0.29",
    "@types/yargs": "17.0.32",
    "ava": "6.1.3",
    "lint-staged": "15.2.5",
    "rimraf": "5.0.7",
    "ts-node": "10.9.2",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0",
    "tsutils": "3.17.1",
    "typescript": "5.4.5",
    "typescript-eslint-parser": "22.0.0"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "execa": "9.1.0",
    "figlet": "1.2.4",
    "glob": "10.4.1",
    "graphback": "^1.0.0",
    "graphql": "16.8.1",
    "graphql-migrations": "^1.0.0",
    "inquirer": "9.2.23",
    "js-yaml": "4.1.0",
    "knex": "3.1.0",
    "node-emoji": "2.1.3",
    "openapi-to-graphql": "3.0.7",
    "parse-github-url": "1.0.2",
    "pg": "8.11.5",
    "request": "2.88.0",
    "sqlite3": "5.1.7",
    "tar": "7.2.0",
    "tmp": "0.1.0",
    "yargs": "17.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
